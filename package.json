{
  "name": "ramblechat-api",
  "version": "1.10.4",
  "description": "RambleChat API for custom client applications",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "clean": "node ./helpers/clean.js",
    "test": "jest",
    "lint": "tslint src/**/*",
    "ts": "tsc -p tsconfig.build.json",
    "build": "run-s clean ts lint",
    "prepublish": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RambleChat/Api"
  },
  "author": "Alexey Svetliakov <asvetliakov@teledini.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "del": "^3.0.0",
    "jest": "^21.0.1",
    "npm-run-all": "^4.1.1",
    "source-map-support": "^0.4.17",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|_[Ss]pec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}