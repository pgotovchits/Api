{
  "name": "@ramble/shared",
  "version": "1.7.5",
  "description": "Shared sources",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "clean": "node ./helpers/clean.js",
    "test": "jest",
    "lint": "tslint src/**/*",
    "ts": "tsc -p tsconfig.build.json",
    "build": "run-s clean ts lint",
    "prepublish": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/SharedSources"
  },
  "author": "Alexey Svetliakov <asvetliakov@teledini.com>",
  "license": "Private",
  "devDependencies": {
    "@ramble/tslint-config": "^1.1.1",
    "@types/jest": "^19.2.3",
    "del": "^2.2.2",
    "jest": "^20.0.3",
    "npm-run-all": "^4.0.2",
    "source-map-support": "^0.4.15",
    "ts-jest": "^20.0.4",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.0.0",
    "tslint-microsoft-contrib": "^5.0.0",
    "tslint-react": "^3.0.0",
    "tsutils": "1.6.0",
    "typescript": "^2.3.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|_[Ss]pec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
